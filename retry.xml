<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name = "Integration Suite">
<listeners>
    <listener class-name="automationPractice.automationPractice.retry.RetryListener"></listener>
</listeners>
    <test name = "Navigation Link">
        <classes>
            <class name = "automationPractice.automationPractice.testscript.TC001_Navigationlink" />
        </classes>
    </test>

    <test name = "SignInRegister">
        <classes>
            <class name = "automationPractice.automationPractice.testscript.TC002_SignInAndRegister" />
        </classes>
    </test>
    <test name = "ContactUs">
        <classes>
            <class name = "automationPractice.automationPractice.testscript.TC_ContactUs" />
        </classes>
    </test>

    <test name = "Shoppingcart">
        <parameter name="categoryname" value="Women"/>
        <parameter name="subCatNameInWomen" value="Tops"/>
        <parameter name="itemName" value="Blouse"/>
        <parameter name="quantity" value="5"/>
        <parameter name="size" value="S"/>
        <classes>
            <class name = "automationPractice.automationPractice.testscript.TC004_ShoppingCart" />
        </classes>
    </test>
</suite>